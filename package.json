    "@commitlint/config-conventional": "^7.5.0",
    "@commitlint/format": "^7.5.0",
    "babel-jest": "^23.6.0",
    "husky": "^2.1.0",
    "jest": "^23.6.0",
    "standard-version": "^5.0.2"
  },
  "peerDependencies": {},
  "keywords": [
    "config",
    "configuration",
    "reducer",
    "simply",
    "simplify",
