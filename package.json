{
  "name": "config-reducer",
  "description": "One config file for development, test and production",
  "version": "2.0.5",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "test": "jest --runInBand --config jest.config.js",
    "test:watch": "yarn test -- --watch",
    "release": "standard-version",
    "alpha": "yarn release -- --prerelease alpha",
    "patch": "yarn release -- --release-as patch",
    "minor": "yarn release -- --release-as minor",
    "major": "yarn release -- --release-as major"
